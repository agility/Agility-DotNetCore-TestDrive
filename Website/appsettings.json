{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Agility": {
    "applicationName": "Agility Test Drive",
    "websiteName": "[WEBSITE NAME]",
    "securityKey": "[YOUR SECURITY KEY]",
    "contentCacheFilePath": "c:/AgilityContent/",
    "contentServerUrl": "https://contentserver-ca.agilitycms.com/agilitycontentserver.svc",
    "developmentMode": "true",
